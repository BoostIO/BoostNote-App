  "build": {
    "appId": "com.electron.Boost",
    "copyright": "Copyright Â© 2016-2017 BoostIO",
    "category": "public.app-category.productivity",
    "files": [
      "app",
      "compiled",
      "resources",
      "node_modules"
    ],
    "asar": true,
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg",
      "icon": "resources/icon.icns"
    }
  }
}
